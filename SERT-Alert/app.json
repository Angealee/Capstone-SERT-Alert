{
  "expo": {
    "name": "SERT Alert",
    "slug": "SERT-Alert",
    "scheme": "com.casptone.sertalert",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icons/serticon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/serticon.png",
      "resizeMode": "contain",
      "width": 300,
      "height": 300,
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capstone.sertalert",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.capstone.sertalert"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "0eae399f-015c-42b4-8888-8c1b52c56870"
      }
    }
  }
}
